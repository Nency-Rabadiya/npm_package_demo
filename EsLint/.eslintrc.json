{
  "env": {
    "browser": true,
    "es2021": true
  },
  // "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["warn", "always"],
    "eqeqeq": "warn",
    "arrow-body-style": "warn",
    "no-else-return": "warn",
    "no-alert": "warn",
    "no-unreachable": "warn"
  }
}
